{
    "name": "@tradevault/server",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc -p tsconfig.json",
        "start": "node dist/server.js",
        "db:migrate": "npx sequelize-cli db:migrate --url sqlite:./database.sqlite --migrations-path migrations"
    },
    "dependencies": {
        "@fastify/cors": "^11.0.0",
        "dotenv": "^16.4.5",
        "fastify": "^5.0.0",
        "sequelize": "^6.37.0",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "sequelize-cli": "^6.6.2",
        "tsx": "^4.19.0",
        "typescript": "^5.6.0"
    }
}